mcdp {
    provides total_load [kg]
    provides velocity [m/s]
    requires power [W]
    requires mass [kg]
   
    constant m₀ = 70 [kg] 
    constant k = 1 [W*s/m/kg] 
    constant p₀ = 100 [W] 

    required power ≥ p₀ + k · (provided total_load + m₀) · provided velocity
    required mass ≥ provided total_load + m₀   
}
