mcdp {
    
    provides total_load [kg]
    provides velocity [m/s]
    requires power [W]

    constant m_0 = 70 [kg] 'self-mass'
    constant k = 1 [W*s/m/kg] 'gain_mass_velocity_to_power'
    constant p_0 = 100 [W] 'min_required_power'

    required power ≥ p_0 + k · (provided total_load + m_0) · provided velocity
  
}
